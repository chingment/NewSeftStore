.specsdialog{
  position: fixed;
  width: 100%;
  left: 0;
  top: 0;
  bottom: 0;
  overflow: hidden;
  background: rgba(0,0,0,.4);
  z-index: 999;
  }
  

  .specsdialog > .content{
    width: 100%;
    height: 800rpx;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 99999;
    background-color: #fff;
  }