  line-height: 64rpx;
}

.its-form .radio {
  display: flex;
  height: 70rpx;
  align-items: center;
}

.its-form .radio image {
  width: 38rpx;
  height: 38rpx;
  line-height: 42rpx;
  display: block;
  float: left;
}

.its-form .radio text {
  margin-left: 15rpx;
  display: block;
  float: left;
}

.btn-bottom {
  display: block;
