/*prd-list-3*/

.prd-list-3 {
  position: relative;
  overflow: hidden;
}

.prd-list-3 .it {
  width: 100%;
  display: flex;
  box-sizing: border-box;
  text-align: center;
  padding: 14rpx 0;
  overflow: hidden;
}

.prd-list-3 .it > .it-left{
  width: 300rpx;
  height: 300rpx;
  display: flex;
}

.prd-list-3 .it > .it-right{
  flex:1;
  display: flex;
}

.prd-list-3 .imgurl {
  text-align: center;
  height: 280rpx;
  width: 280rpx;
  margin: auto;
  position: relative;
  overflow: hidden;
}

.prd-list-3 .name {
  width: 100%;
  height: 44rpx;
  line-height: 44rpx;
  font-size: 28rpx;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  color: #000;
  margin:10rpx 0;
}

.prd-list-3 .briefInfo {
  width: 100%;
  line-height: 42rpx;
  font-size: 24rpx;
  text-align: left;
  display: -webkit-box;
  margin:10rpx 0;
}

.prd-list-3 .area2 {
  display: flex;
}

.prd-list-3 .price {
  flex: 1;
  text-align: left;
  margin-right:10px;
  
}

.prd-list-3 .rentMhPrice {
  height: 62rpx;
  line-height: 62rpx;
  font-size: 42rpx;
  display: inline-block;
  color: #ffb101;
}

.prd-list-3 .rentMhPriceUnit {
  height: 32rpx;
  line-height: 32rpx;
  font-size: 32rpx;
  display: inline-block;
  color: #ffb101;
}

.prd-list-3 .showprice {
  margin-left: 10rpx;
  height: 62rpx;
  line-height: 62rpx;
  font-size: 32rpx;
  display: inline-block;
  text-decoration: line-through;
}

.prd-list-3 .operate {
  flex: 1;
  text-align: right;
  align-items: center;
  justify-content: flex-end;
  height: 62rpx;
  display: flex;
}

.prd-list-3 .decrease {
  width: 50rpx;
  height: 50rpx;
}

.prd-list-3 .quantity {
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
}

.prd-list-3 .increase {
  width: 50rpx;
  height: 50rpx;
}

