/* pages/goods-mgr/brand/index.wxss */
@import "../common.wxss";

.goodsMgr-brand {
  padding-bottom: 108rpx;
  min-height: 100vh;
  background-color: #fff;
}

::-webkit-scrollbar {
  display: none;
}

/* 对话框样式 */
.modal-body {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 280rpx;
}

.modal-title {
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 40px;
}

.placeholder-cls {
  margin-left: -40rpx;
  text-align: center;
}

